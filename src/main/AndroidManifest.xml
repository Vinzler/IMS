<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.imsproject">

    <application
        android:allowBackup="true"
        android:icon="@drawable/applogo"
        android:label="IMS"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".TrialActivity"></activity>
        <activity android:name=".ViewideaActivity" />
        <activity android:name=".Viewsubmittedideas" />
        <activity android:name=".SubmitideaActivity" />
        <activity
            android:name=".NewprofileActivity"
            android:label="Idea Feed"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".ProfileActivity" />
        <activity android:name=".SignupActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>